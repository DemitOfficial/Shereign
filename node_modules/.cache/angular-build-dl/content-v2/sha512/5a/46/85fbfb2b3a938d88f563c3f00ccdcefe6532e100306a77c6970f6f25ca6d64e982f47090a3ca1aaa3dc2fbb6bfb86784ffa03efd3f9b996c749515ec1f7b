, percentage) {\n    if (e.target.checked && this.discountRates.length === 0) {\n      this.products = productList.filter((product) => {\n        return product.discount < percentage;\n      });\n    }\n    else {\n      this.products = productList.filter((product) => {\n        return product.discount >= Math.max.apply(null, this);\n      }, this.discountRates);\n    }\n  }\n\n  discountMoreFilter(e, percentage: number) {\n    if (e.target.checked) {\n      this.discountRates.push(percentage);\n    } else {\n      this.discountRates.splice(this.discountRates.indexOf(percentage), 1);\n    }\n    this.products = productList.filter((product) => {\n      return product.discount >= Math.max.apply(null, this);\n    }, this.discountRates);\n  }\n\n  valueChange(value: number, boundary: boolean): void {\n    if (boundary) {\n      this.minVal = value;\n    } else {\n      this.maxVal = value;\n      this.products = productList.filter(function (product) {\n        return product.disRate <= value && product.disRate >= this;\n      }, this.minVal);\n    }\n  }\n\n}\n","<div class=\"container-fluid\">\n    <!-- page title -->\n    <app-page-title title=\"Products\" [breadcrumbItems]=\"breadCrumbItems\"></app-page-title>\n\n    <div class=\"row\">\n        <div class=\"col-xl-3 col-lg-4\">\n\n            <div class=\"card\">\n                <div class=\"card-header bg-transparent border-bottom\">\n                    <h5 class=\"mb-0\">Filters</h5>\n                </div>\n\n                <div class=\"p-4\">\n                    <h5 class=\"font-size-14 mb-3\">Categories</h5>\n                    <div class=\"custom-accordion\">\n                        <a class=\"text-body fw-semibold pb-2 d-block\" (click)=\"collapse.toggle()\"\n                            href=\"javascript:void(0);\" role=\"button\" aria-expanded=\"false\"\n                            aria-controls=\"categories-collapse\">\n                            <i class=\"mdi mdi-chevron-up accor-down-icon text-pri