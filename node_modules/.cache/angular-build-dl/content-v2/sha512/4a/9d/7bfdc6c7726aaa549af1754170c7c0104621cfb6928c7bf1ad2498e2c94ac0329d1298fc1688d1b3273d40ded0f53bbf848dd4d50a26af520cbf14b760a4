e"),o["\u0275\u0275text"](306,"disabled"),o["\u0275\u0275elementEnd"](),o["\u0275\u0275text"](307," attribute."),o["\u0275\u0275elementEnd"](),o["\u0275\u0275elementStart"](308,"div",2),o["\u0275\u0275elementStart"](309,"div",54),o["\u0275\u0275elementStart"](310,"div"),o["\u0275\u0275elementStart"](311,"h5",119),o["\u0275\u0275text"](312,"Switch examples"),o["\u0275\u0275elementEnd"](),o["\u0275\u0275elementStart"](313,"div",120),o["\u0275\u0275element"](314,"input",121),o["\u0275\u0275elementStart"](315,"label",122),o["\u0275\u0275text"](316,"Toggle this switch element"),o["\u0275\u0275elementEnd"](),o["\u0275\u0275elementEnd"](),o["\u0275\u0275elementStart"](317,"div",123),o["\u0275\u0275element"](318,"input",124),o["\u0275\u0275elementStart"](319,"label",125),o["\u0275\u0275text"](320,"Disabled switch element"),o["\u0275\u0275elementEnd"](),o["\u0275\u0275elementEnd"](),o["\u0275\u0275elementEnd"](),o["\u0275\u0275elementEnd"](),o["\u0275\u0275elementStart"](321,"div",54),o["\u0275\u0275elementStart"](322,"div",126),o["\u0275\u0275elementStart"](323,"h5",119),o["\u0275\u0275text"](324,"Switch sizes"),o["\u0275\u0275elementEnd"](),o["\u0275\u0275elementStart"](325,"div",120),o["\u0275\u0275element"](326,"input",127),o["\u0275\u0275elementStart"](327,"label",128),o["\u0275\u0275text"](328,"Small Size Switch"),o["\u0275\u0275elementEnd"](),o["\u0275\u0275elementEnd"](),o["\u0275\u0275elementStart"](329,"div",129),o["\u0275\u0275element"](330,"input",130),o["\u0275\u0275elementStart"](331,"label",131),o["\u0275\u0275text"](332,"Medium Size Switch"),o["\u0275\u0275elementEnd"](),o["\u0275\u0275elementEnd"](),o["\u0275\u0275elementStart"](333,"div",132),o["\u0275\u0275element"](334,"input",133),o["\u0275\u0275elementStart"](335,"label",134),o["\u0275\u0275text"](336,"Large Size Switch"),o["\u0275\u0275elementEnd"](),o["\u0275\u0275elementEnd"](),o["\u0275\u0275elementEnd"](),o["\u0275\u0275elementEnd"](),o["\u0275\u0275elementEnd"](),o["\u0275\u0275elementEnd"](),o["\