        }, {
          key: "setColorMode",
          value: function setColorMode(mode) {
            switch (mode.toString().toUpperCase()) {
              case '1':
              case 'C':
              case 'COLOR':
                this.cpColorMode = 1;
                break;

              case '2':
              case 'G':
              case 'GRAYSCALE':
                this.cpColorMode = 2;
                break;

              case '3':
              case 'P':
              case 'PRESETS':
                this.cpColorMode = 3;
                break;

              default:
                this.cpColorMode = 1;
            }
          }
        }, {
