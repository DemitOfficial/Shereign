      var ColorFormats;

      (function (ColorFormats) {
        ColorFormats[ColorFormats["HEX"] = 0] = "HEX";
        ColorFormats[ColorFormats["RGBA"] = 1] = "RGBA";
        ColorFormats[ColorFormats["HSLA"] = 2] = "HSLA";
        ColorFormats[ColorFormats["CMYK"] = 3] = "CMYK";
      })(ColorFormats || (ColorFormats = {}));

