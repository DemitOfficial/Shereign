      var RepeaterComponent = /*#__PURE__*/function () {
        function RepeaterComponent(fb) {
          _classCallCheck(this, RepeaterComponent);

          this.fb = fb;
          this.form = this.fb.group({
            formlist: this.fb.array([])
          }), this.phoneData = this.fb.group({
            phoneValue: this.fb.array([])
          });
        }

        _createClass(RepeaterComponent, [{
          key: "ngOnInit",
          value: function ngOnInit() {
            this.breadCrumbItems = [{
              label: 'Forms'
            }, {
              label: 'Repeater',
              active: true
            }];
          }
        }, {
          key: "formData",
          value: function formData() {
            return this.form.get('formlist');
          }
        }, {
          key: "phonedata",
          value: function phonedata() {
            return this.phoneData.get('phoneValue');
          }
        }, {
          key: "phone",
          value: function phone() {
            return this.fb.group({
              phonenumber: ''
            });
          }
        }, {
          key: "field",
          value: function field() {
            return this.fb.group({
              name: '',
              email: '',
              subject: '',
              file: '',
              msg: ''
            });
          }
          /**
           * Add phone field in list
           */

        }, {
          key: "addPhone",
          value: function addPhone() {
            this.phonedata().push(this.phone());
          }
          /**
           * Remove field from form
           * @param i specified index to remove
           */

        }, {
          key: "removeField",
          value: function removeField(i) {
            if (confirm('Are you sure you want to delete this element?')) {
              this.formData().removeAt(i);
            }
          }
          /**
           * Delete phone field from list
           * @param i specified index
           */

        }, {
          key: "deletePhone",
          value: function deletePhone(i) {
            this.phonedata().removeAt(i);
          }
          /**
           * Add field in form
           */

        }, {
          key: "addField",
          value: function addField() {
            this.formData().push(this.field());
          }
        }]);

        return RepeaterComponent;
      }();

