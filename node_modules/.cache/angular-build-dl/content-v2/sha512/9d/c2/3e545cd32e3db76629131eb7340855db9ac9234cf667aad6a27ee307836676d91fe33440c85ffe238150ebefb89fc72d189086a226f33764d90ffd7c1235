                      continue;
                    }
                  }

                  if (maskExpression[cursor] === 'h') {
                    if (result === '2' && Number(inputSymbol) > 3) {
                      cursor += 1;
                      _i4--;
                      continue;
                    }
                  }

                  if (maskExpression[cursor] === 'm') {
                    if (Number(inputSymbol) > 5) {
                      cursor += 1;

