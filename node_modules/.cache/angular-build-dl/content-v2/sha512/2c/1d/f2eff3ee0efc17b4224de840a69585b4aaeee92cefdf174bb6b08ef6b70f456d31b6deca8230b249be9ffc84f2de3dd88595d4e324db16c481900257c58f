        props.sideText && (Object(_fullcalendar_common__WEBPACK_IMPORTED_MODULE_1__["createElement"])("a", Object(tslib__WEBPACK_IMPORTED_MODULE_2__["__assign"])({ className: "fc-list-day-side-text" }, navLinkAttrs), props.sideText))));
